    "README.md"
  ],
  "author": "Victor Zamfir <zamfir.victor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viczam/event-dispatcher/issues"
  },
  "homepage": "https://github.com/viczam/event-dispatcher#readme",
  "dependencies": {
    "joi": "^8.0.5",
    "lodash": "^4.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "5.0.0",
    "babel-plugin-transform-async-to-generator": "6.7.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.0",
    "mocha": "^2.4.5",
    "rimraf": "2.5.2",
    "sinon": "^1.17.3"
  },
  "babel": {
    "presets": ["es2015", "stage-0"],
    "plugins": ["transform-object-rest-spread", "transform-async-to-generator", "transform-runtime"]
