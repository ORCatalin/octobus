{
  "name": "octobus.js",
  "version": "0.0.1",
  "description": "An event dispatcher aimed for AOP.",
  "main": "dist/index.js",
  "scripts": {
    "watch:test": "npm t -- -w",
    "test": "mocha --compilers js:babel-register --require ./test/helper.js",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viczam/event-dispatcher.git"
  },
  "keywords": [
